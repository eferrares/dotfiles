alias ga='git add'
alias gau='git add -u'
alias gb='git branch'
alias gc='git checkout'
alias gcl='git clone'
alias gc='git commit'
alias gcm='gc -m'
alias gcma='gc -am'
alias gcv='gc -v'
alias gd='git diff'
alias gl='git log'
alias glgol='gl --graph --decorate --pretty=oneline --abbrev-commit'
alias glgola='gl --graph --decorate --pretty=oneline --abbrev-commit --all'
alias gps='git push'
alias gpl='git pull'
alias gra='git remote add'
alias grm='git rm'
alias grr='git remote rm'
alias gst='git status'